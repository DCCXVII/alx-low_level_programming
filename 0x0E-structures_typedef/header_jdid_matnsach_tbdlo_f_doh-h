#ifndef DOG_H
#define DOG_H

/**
 * struct dog - structure of a dog
 * @name: name of the dog
 * @age: age of the dog
 * @owner: owner of the dog
 *
 * Description: structure called dog containing
 * name, age, owner of a dog
 */

typedef struct dog
{
	char *name;
	float age;
	char *owner;
} dog_t;

/* Function prototypes */
void init_dog(struct dog_t *d, char *name, float age, char *owner);
void print_dog(struct dog_t *d);

#endif /* DOG_H */ 
